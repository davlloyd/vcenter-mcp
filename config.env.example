# vCenter Configuration Template
# Copy this file to .env and replace the values with your actual vCenter server details

# Required: vCenter server host URL
VCENTER_HOST=https://your-vcenter-server.com

# Required: vCenter username
VCENTER_USERNAME=your-username

# Required: vCenter password
VCENTER_PASSWORD=your-password

# Optional: SSL verification (default: true)
# Set to false if using self-signed certificates
VCENTER_VERIFY_SSL=true

# Optional: Request timeout in seconds (default: 30)
VCENTER_TIMEOUT=30

# Optional: Log level for debugging (default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO 